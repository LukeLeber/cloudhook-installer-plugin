{
    "name": "lleber/cloudhook-installer-plugin",
    "description": "A composer installer plugin for Acquia cloudhooks.",
    "type": "composer-plugin",
    "require": {
        "composer/composer": "^1.8",
        "composer-plugin-api": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Leber",
            "email": "lukeleber@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "lleber\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lleber\\Tests\\": "tests/src"
        }
    },
    "extra": {
        "class": "lleber\\Composer\\CloudhookPlugin"
    }
}
